            <label>Nombre completo</label>
            <input class="form-control" id="NombreEnvia" placeholder="Ingrese nombre">
        </div>
        <div class="form-group">
            <label>Direcciòn</label>
            <input class="form-control" id="DireccionEnvia" placeholder="Ingrese direccion">
        </div>
        <div class="form-group">
            <label>Numero telefonico</label>
